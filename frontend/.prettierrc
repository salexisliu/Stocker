{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "printWidth": 120,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["*.tsx", "*.jsx"],
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "htmlWhitespaceSensitivity": "ignore"
}
